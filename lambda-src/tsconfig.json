{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES6",
        "outDir": "dist",
        "noImplicitAny": true,
        "allowSyntheticDefaultImports": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true
    },
    "exclude": [
        "node_modules",
        "dist"
    ]    
}